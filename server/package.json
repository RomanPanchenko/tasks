{
  "name": "tasks-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.12.0",
    "npm": ">=10.5.0"
  },
  "scripts": {
    "build": "rimraf dist && nest build",
    "build:clean": "rimraf node_modules dist package-lock.json && npm i && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "npx prisma generate",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "npm run prisma:generate & cross-env NODE_ENV=development nest start --debug=127.0.0.1:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "npm run test:e2e",
    "test:e2e": "npm run migrate:test && cross-env NODE_ENV=test jest --runInBand --ci --logHeapUsage --no-cache --detectOpenHandles --forceExit --coverage",
    "test:unit": "jest --runInBand --ci --logHeapUsage --no-cache --detectOpenHandles --forceExit --coverage",
    "migration:generate": "typeorm migration:generate ./src/_migrations/unknown -d ./ormconfig.js",
    "migrate:development": "npm run build && cross-env NODE_ENV=development node ./src/_common/pre-db-migration.js && cross-env NODE_ENV=development typeorm migration:run -d ./ormconfig.js",
    "migrate:test": "npm run build && cross-env NODE_ENV=test node ./src/_common/pre-db-migration.js && cross-env NODE_ENV=test typeorm migration:run -d ./ormconfig.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.740.0",
    "@nestjs/apollo": "^13.0.3",
    "@nestjs/cache-manager": "3.0.0",
    "@nestjs/common": "11.0.7",
    "@nestjs/core": "11.0.7",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/platform-express": "11.0.7",
    "@nestjs/schedule": "5.0.1",
    "@nestjs/swagger": "11.0.3",
    "@nestjs/typeorm": "11.0.0",
    "@prisma/client": "^6.5.0",
    "apollo-server-express": "^3.13.0",
    "axios": "1.7.9",
    "cache-manager": "6.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "config": "3.3.12",
    "cookie-parser": "1.4.7",
    "express": "4.21.2",
    "express-mysql-session": "3.0.3",
    "express-session": "1.18.1",
    "graphql": "^16.10.0",
    "helmet": "8.0.0",
    "joi": "17.10.2",
    "moment": "2.30.1",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.11.0",
    "nest-winston": "1.10.2",
    "prisma": "^6.5.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "safe-stable-stringify": "2.5.0",
    "swagger-ui-express": "5.0.1",
    "ulid": "^2.3.0",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0",
    "winston-transport": "4.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.2",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.7",
    "@stylistic/eslint-plugin": "2.8.0",
    "@types/express": "4.17.18",
    "@types/express-session": "1.17.8",
    "@types/jest": "29.5.13",
    "@types/multer": "1.4.7",
    "@types/node": "20.14.0",
    "@types/supertest": "2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "cross-env": "7.0.3",
    "eslint": "9.19.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "nyc": "17.1.0",
    "prettier": "3.0.3",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
